  <h1>予防接種詳細ページ</h1>
  <div class="text-center"> 

  <strong>ワクチン名：</strong> <%= @vaccine.name %>
      <strong>接種回数：</strong> <%= @vaccine.count %>
      <strong>接種時期：</strong>  <%= @vaccine.expected_date %>
  <% if @check.present? %>
    <%= link_to 'お気に入り解除する', check_path(id: @check.id), method: :delete, class: 'btn btn-danger' %>
  <% else %>
    <%= link_to 'お気に入りする', checks_path(vaccine_id: @vaccine.id), method: :post, class: 'btn btn-primary' %>
  <% end %>
  <strong>接種日：</strong>  <%= @vaccine.inplemented_date %></p>

